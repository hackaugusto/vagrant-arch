[statsite]
port=8125
udp_port=8125
log_level=DEBUG
flush_interval=10
timer_eps=0.01
set_eps=0.02
stream_cmd=/usr/bin/python2 /usr/bin/statsite_graphite.py {{ hostvars['main']['ansible_eth0']['ipv4']['address'] }} 2003

[histogram_api]
prefix=api
min=0
max=100
width=5

[histogram_default]
prefix=
min=0
max=200
width=20
